#include<stdio.h>
int main(){
    char str1[100], str2[100];
    int count[256] = {0}, i;
    printf("Enter first string: ");
    scanf("%s", str1);
    printf("Enter second string: ");
    scanf("%s", str2);  
    for(i = 0; str1[i] && str2[i]; i++) {
        count[(int)str1[i]]++;
        count[(int)str2[i]]--;
    }
    for(i = 0; i < 256; i++) {
        if(count[i] != 0) {
            printf("Not an anagram\n");
            return 0;
        }
    }
    printf("Anagram\n");
    return 0;
}
